  "name": "@triply/facetcheck",
  "version": "1.0.47",
  "main": "build/src/index.js",
  "author": "Laurens Rietveld <laurens.rietveld@triply.cc>",
  "typings": "build/src/index.d.ts",
  "scripts": {
    "build": "npm run prod:build",
    "dev:watch": "better-npm-run watch",
    "dev": "npm run util:clean  && npm run dev:watch",
    "postpublish": "versionToBranch",
    "prepublishOnly": "npm run prod:build",
    "prod:build": "npm run util:clean && better-npm-run build && tsc  -p tsconfig-build.json",
    "prod:start": "better-npm-run start",
    "upgrade-interactive": "npm-check --skip-unused -u",
    "util:buildWebpackConfig": "tsc -p tsconfig-webpack.json",
    "util:clean": "rm -rf build && mkdir -p build",
    "util:lint": "tslint --project tsconfig-build.json -c tslint-hook.json 'src/**/*.ts?'"
  },
  "betterScripts": {
    "start": {
      "command": "node ./bin/server.js",
      "env": {
        "NODE_ENV": "production",
        "PORT": 5000
      }
    },
    "watch": {
      "command": "npm run util:buildWebpackConfig && webpack-dev-server --disable-host-check --public 0.0.0.0 --host 0.0.0.0 --port 5000 --config ./webpack/config.js",
      "env": {
        "UV_THREADPOOL_SIZE": 100,
        "NODE_ENV": "development",
        "NODE_TLS_REJECT_UNAUTHORIZED": "0"
      }
    },
    "build": {
      "command": "npm run util:buildWebpackConfig &&  webpack --config ./webpack/config.js --progress --profile --colors",
      "env": {
        "NODE_ENV": "production"
      }
    }
  },
  "repository": {
